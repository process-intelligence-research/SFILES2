   "source": [
    "# Create SFILES from graph using the flowsheet class\n",
    "flowsheet_1.convert_to_sfiles(version='v2')\n",
    "sfiles_1 = flowsheet_1.sfiles\n",
    "print(sfiles_1)\n",
    "flowsheet_1.create_from_sfiles(sfiles_1, override_nx=True)\n",
    "flowsheet_1.convert_to_sfiles(version='v2')\n",
